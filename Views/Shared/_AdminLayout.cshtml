@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/TemplateUI/plugins/fontawesome-free/css/all.min.css">
    <!-- IonIcons -->
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/TemplateUI/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar elevation-4" style="background-color: #C70039">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="~/TemplateUI/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light" style="color: white">AdminLTE 3</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/TemplateUI/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block" style="color: white">Yasir Kurt</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
    with font-awesome or any other icon font library -->
                        <li class="nav-item has-treeview">
                            <a href="/AdminCategory/Index" class="nav-link btn-a">
                                <i class="nav-icon fas fa-tachometer-alt" style="color: white"></i>
                                <p style="color: white">
                                    Kategoriler
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Heading/Index" class="nav-link">
                                <i class="nav-icon fas fa-th" style="color: white"></i>
                                <p style="color: white">
                                    Başlıklar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Writer/Index" class="nav-link">
                                <i class="nav-icon fas fa-copy" style="color: white"></i>
                                <p style="color: white">
                                    Yazarlar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/About/Index" class="nav-link">
                                <i class="nav-icon fas fa-tree" style="color: white"></i>
                                <p style="color: white">
                                    Hakkımızda
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Contact/Index" class="nav-link">
                                <i class="nav-icon fas fa-edit" style="color: white"></i>
                                <p style="color: white">
                                    İletişim & Mesajlar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-table" style="color: white"></i>
                                <p style="color: white">
                                    Yazılar
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="pages/calendar.html" class="nav-link">
                                <i class="nav-icon fas fa-calendar-alt" style="color: white"></i>
                                <p style="color: white">
                                    Yetkilendirmeler
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Gallery/Index" class="nav-link">
                                <i class="nav-icon far fa-image" style="color: white"></i>
                                <p style="color: white">
                                    Galeri
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-chart-pie" style="color: white"></i>
                                <p style="color: white">
                                    Grafikler
                                    <i class="right fas fa-angle-left" style="color: white"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="pages/charts/chartjs.html" class="nav-link">
                                        <i class="far fa-circle nav-icon ml-4" style="color: white"></i>
                                        <p style="color: white">Grafik 1</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="pages/charts/flot.html" class="nav-link">
                                        <i class="far fa-circle nav-icon ml-4" style="color: white"></i>
                                        <p style="color: white">Grafik 2</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="pages/charts/inline.html" class="nav-link">
                                        <i class="far fa-circle nav-icon ml-4" style="color: white"></i>
                                        <p style="color: white">Grafik 3</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/ErrorPage/Page404" class="nav-link">
                                <i class="nav-icon far fa-envelope" style="color: white"></i>
                                <p style="color: white">
                                    Hata Sayfaları
                                    <i class="fas fa-angle-left right" style="color: white"></i>
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Statistic/Index" class="nav-link">
                                <i class="nav-icon fas fa-book" style="color: white"></i>
                                <p style="color: white">
                                    İstatistikler
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-book" style="color: white"></i>
                                <p style="color: white">
                                    Siteye Git
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon far fa-times-circle" style="color: white"></i>
                                <p style="color: white">
                                    Çıkış Yap
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <div class="content">
                @RenderBody()
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
   <script src="~/TemplateUI/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/TemplateUI/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE -->
    <script src="~/TemplateUI/dist/js/adminlte.js"></script>

    <!-- OPTIONAL SCRIPTS -->
    <script src="~/TemplateUI/plugins/chart.js/Chart.min.js"></script>
    <script src="~/TemplateUI/dist/js/demo.js"></script>
    <script src="~/TemplateUI/dist/js/pages/dashboard3.js"></script>
</body>
</html>